<link rel="shortcut icon" href="~/assets/media/logos/favicon.ico" />

<div class="content  d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-4  subheader-solid " id="kt_subheader">
        <div class=" container-fluid  d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-2">

                <!--begin::Page Title-->
                <h5 class="text-dark font-weight-bold mt-2 mb-2 mr-5">
                    Details
                </h5>
                <!--end::Page Title-->
                <!--begin::Actions-->
                <div class="subheader-separator subheader-separator-ver mt-2 mb-2 mr-4 bg-gray-200"></div>

                <span class="text-muted font-weight-bold mr-4">Project</span>

                @*<a href="#" class="btn btn-light-warning font-weight-bolder btn-sm">
                    Add New
                    </a>*@
                <!--end::Actions-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <div class="d-flex align-items-center">
                <!--begin::Actions-->
                <a href="#" class=" btn-sm font-weight-bold font-size-base mr-1">
                    @DateTime.Today.ToLongDateString()
                </a>
                <!--end::Actions-->

            </div>
            <!--end::Toolbar-->
        </div>
    </div>
    <div class="card-custom">
        <div class="card-header flex-wrap border-0 pt-6 pb-0">
            <div class="card-title">
                <h3 class="card-label">
                    Details
                    <span class="d-block text-muted pt-2 font-size-sm">Project Detail Information</span>
                </h3>
            </div>
            <div class="card-toolbar">
            </div>
        </div>

        <div class="card-body">
            <div>
                <h4>Project</h4>
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProjectId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ProjectId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProjectName)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ProjectName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ChartOfAccount)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ChartOfAccount)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Remark)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Remark)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CreationTime)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CreationTime)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CreatorUserId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CreatorUserId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.LastModificationTime)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.LastModificationTime)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.LastModifierUserId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.LastModifierUserId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.IsDeleted)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.IsDeleted)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.DeletionTime)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.DeletionTime)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.DeleterUserId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.DeleterUserId)
                    </dd>
                </dl>
            </div>
        </div>
    </div>

</div>



<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>

